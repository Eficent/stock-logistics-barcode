<odoo>
    <record id="view_wizard_print_product_label_form" model="ir.ui.view">
        <field name="name">wizard.print.record.label.form</field>
        <field name="model">wizard.print.record.label</field>
        <field
            name="inherit_id"
            ref="printer_zpl2.view_wizard_print_product_label_form"
        />
        <field name="arch" type="xml">
            <xpath expr="//group" position="after">
                <field name="line_ids">
                    <tree string="Labels Configuration" editable="bottom">
                        <field name="label_header_id" invisible="1" />
                        <field name="product_id" />
                        <field name="label_no" />
                        <field
                            name="package_barcode"
                            groups="stock.group_tracking_lot"
                        />
                        <field name="lot_barcode" groups="stock.group_production_lot" />
                        <field name="product_qty" />
                        <field name="uom" />
                    </tree>
                </field>
            </xpath>
        </field>
    </record>
</odoo>
